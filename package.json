{
  "name": "ubuntu-maas",
  "version": "0.0.1",
  "description": "Static Django website blueprint ===",
  "main": "index.js",
  "scripts": {
    "test": "sass-lint static/**/*.scss --verbose --no-exit",
    "clean": "rm -rf node_modules yarn-error.log",
    "watch": "node-sass --include-path node_modules --watch --source-map true --recursive static/sass/styles.scss static/css/styles.css",
    "build": "node-sass --include-path node_modules static/sass --output static/css "
  },
  "keywords": [
    "maas",
    "ubuntu"
  ],
  "author": "Canonical webteam",
  "license": "LGPL v3",
  "devDependencies": {
    "sass-lint": "1.10.2",
    "cloud-vanilla-theme": "0.1.2",
    "node-sass": "4.5.2"
  }
}
